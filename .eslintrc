{
  "extends": ["@nuxtjs/eslint-config-typescript","plugin:typescript-sort-keys/recommended"],
  "plugins": ["typescript-sort-keys", "sort-keys-fix"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "sort-keys-fix/sort-keys-fix": "error",
    "quotes": [
      "error",
      "double"
  ],
  "no-console": "error"
}
}